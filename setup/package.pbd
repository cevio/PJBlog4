			function object2array(object){
				var arr1 = [];
				for ( var i = 0 ; i < object.length ; i++ ){
					var arr2 = [];
					for ( var j = 0 ; j < object[i].length ; j++ ){
						var tmp_data = object[i][j];
						if ( typeof tmp_data === "date" ){
							tmp_data = (new Date( tmp_data )).getTime();
