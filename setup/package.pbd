	function cacheTemplateFile( context ){
		var percent = "%";
		return '<' + percent + '\ndefine(function(){ \n    return ' + context + ';\n});\n' + percent + '>';
	}
	
	if ( cacheMode === undefined ){
		console.push(selector(cacheHandle) + " 模块未知，请确认该模块是否存在或者符合规范。");
		return null;
	}else{
		return (function(dataList){
